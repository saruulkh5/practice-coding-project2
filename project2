   ],
   "source": [
    "q4 = '''SELECT f1.name as min_pop, f2.name as max_pop \n",
    "FROM facts f1, facts f2 \n",
    "WHERE f1.population == 0 and f2.population = (\n",
    "SELECT max(population) FROM facts)'''\n",
    "pd.read_sql_query(q4,conn)"
   ]
  },
