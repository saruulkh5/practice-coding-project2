    "# Query to find water-to-land ratio excluding the outliers on the right tail\n",
    "q10 = '''\n",
    "SELECT cast(area_water as float)/cast(area as float) as water_land_ratio\n",
    "FROM facts\n",
    "WHERE water_land_ratio < 0.1\n",
    "'''\n",
    "pd.read_sql_query(q10, conn)\n",
    "\n",
    "# Draw the histogram\n",
    "fig = plt.figure(figsize=(10,10))\n",
    "ax = fig.add_subplot(111)\n",
    "pd.read_sql_query(q10, conn).hist(ax=ax)"
   ]
