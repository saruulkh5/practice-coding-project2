   "source": [
    "q3 = '''SELECT min(population) as min_pop, \n",
    "max(population) as max_pop, \n",
    "min(population_growth) as min_grow,\n",
    "max(population_growth) as max_grow FROM facts'''\n",
    "\n",
    "pd.read_sql_query(q3, conn)"
   ]
  },
